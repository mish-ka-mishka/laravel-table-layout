{
    "name": "mkaverin/laravel-table-layout",
    "description": "This package provides a simple way to display a resource as a table for your Laravel application.",
	"homepage": "https://github.com/mish-ka-mishka/laravel-table-layout",
    "autoload": {
        "psr-4": {
            "TableLayout\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Michael",
            "email": "m@kaverin.ml"
        }
    ],
	"extra": {
		"laravel": {
			"providers": [
				"TableLayout\\Providers\\TableLayoutServiceProvider"
			]
		}
	},
    "require": {
	    "php": "^7.4|^8.0",
	    "illuminate/contracts": "^8.0|^9.0",
	    "illuminate/support": "^8.0|^9.0",
	    "illuminate/http": "^8.0|^9.0"
    },
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
